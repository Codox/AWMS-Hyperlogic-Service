{
  "name": "awms-hyperlogic-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migrate:show": "./node_modules/.bin/typeorm-ts-node-commonjs migration:show -d src/ormconfig.ts",
    "migrate:run": "./node_modules/.bin/typeorm-ts-node-commonjs migration:run -d src/ormconfig.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit:local": "jest --maxWorkers=50% --config jest-unit.json",
    "test:unit:watch": "jest --watch --maxWorkers=25% --config jest-unit.json",
    "test:unit:ci": "jest --runInBand --config jest-unit.json",
    "test:e2e:local": "cd test/e2e && ./run-local.sh",
    "test:e2e:ci": "cd test/e2e && ./run-ci.sh",
    "test:e2e": "jest --maxWorkers=50% --config jest-e2e.json",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@fastify/static": "^6.10.2",
    "@nestjs/axios": "^3.0.0",

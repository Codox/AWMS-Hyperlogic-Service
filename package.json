    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "countries-list": "^2.6.1",
    "i18n-iso-countries": "^7.6.0",
    "keycloak-connect": "^21.1.2",
    "libphonenumber-js": "^1.10.28",
    "lodash": "^4.17.21",
    "nest-keycloak-connect": "^1.9.2",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-mixer": "^6.0.3",
    "typeorm": "^0.3.15"
